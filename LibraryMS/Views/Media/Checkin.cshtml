
@{
    ViewData["Title"] = "Checkin";
}

<h2>Checkin</h2>
<form style="margin-top: 20px;" class="w3-container" asp-controller="Media" asp-action="Checkin" method="get">
    <h3>Choose the appropriate date range:</h3>

    <div style="margin-top: 15px;" class="form-row">
        <div class="col-sm-2">
            <label>
                Username:
            </label>
        </div>
        <div class="col">
            <input type="text" name="UserName" /><!--value="@ViewBag.fromTime"> -->
        </div>
    </div>
    <div style="margin-top: 15px;" class="form-row">
        <div class="col-sm-2">
            <label>
                Media ID:
            </label>
        </div>
        <div class="col">
            <input type="text" name="MediaID" /><!--value="@ViewBag.fromTime"> -->
        </div>
    </div>

    <div class="form-row">
        <div class="col">
            <input type="submit" class="btn btn-primary" id="filter-button">
           <!-- <button class="btn btn-default" type="submit" style="margin-top:15px; margin-bottom: 15px">
               <!--- @Html.ActionLink("Back to View", "Index", "Reports", null)
            </button> -->
        </div>
    </div>
</form>